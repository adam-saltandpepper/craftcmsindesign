{% set entries = craft.entries.section('fastFacts') %}
{
  "data": [
    {% for entry in entries %}
    {
      "title": "{{entry.title}}",
      "entryId": {{entry.id}},
      "entryDate": "{{entry.postDate}}",
      "jsonUrl": "{{entry.url}}"
    }{% if not loop.last %},{% endif %}
    {% endfor %}
  ]
}